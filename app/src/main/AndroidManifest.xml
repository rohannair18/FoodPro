<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.foodpro">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/foodprofinallogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/foodprofinallogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.FoodPro">
        <activity
            android:name=".Restaurant"
            android:exported="false" />
        <activity android:name=".Details" />
        <activity android:name=".CheckOut" />
        <activity android:name=".Location" />
        <activity android:name=".Order" />
        <activity android:name=".Restaurants" />
        <activity android:name=".Register" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>